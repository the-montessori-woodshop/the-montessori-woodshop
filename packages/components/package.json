{
  "name": "@woodshop/components",
  "version": "1.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "theme:build": "sass src/theme/index.scss css/theme.css",
    "types": "tsc",
    "bundle": "rollup --config rollup.config.cjs",
    "build": "yarn bundle && yarn theme:build && yarn types",
    "build:npm": "npm run bundle && npm run types & npm run theme:build",
    "dev": "yarn bundle --watch & yarn theme:build --watch"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@woodshop/builder": "file:../builder",
    "clsx": "^1.1.1",
    "eslint": "8.7.0",
    "eslint-plugin-react": "^7.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.64.0",
    "sass": "^1.49.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
