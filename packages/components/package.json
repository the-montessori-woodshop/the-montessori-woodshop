{
  "name": "@woodshop/components",
  "version": "1.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "theme:build": "sass src/theme/index.scss css/theme.css",
    "types": "tsc",
    "bundle": "rollup --config rollup.config.cjs",
    "build": "yarn bundle && yarn theme:build && yarn types",
    "build:npm": "npm run bundle && npm run types & npm run theme:build",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider && build-storybook",
    "dev": "yarn bundle --watch & yarn storybook & yarn theme:build "
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-docs": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.9",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@woodshop/builder": "file:../builder",
    "babel-loader": "^8.2.3",
    "clsx": "^1.1.1",
    "css-loader": "^6.5.1",
    "eslint": "8.6.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.5.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.63.0",
    "sass": "^1.47.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0"
  },
  "peerDependencies": {
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
