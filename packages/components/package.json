{
  "name": "@woodshop/components",
  "version": "1.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "main": "./dist/index.cjs",
      "module": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./font/*": {
      "import": "./dist/fonts/*",
      "require": "./dist/fonts/*"
    },
    "./styles": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "scripts": {
    "types": "tsc --project tsconfig.types.json",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "dev": "yarn node --loader ts-node/esm ./scripts/build.dev.ts & yarn types --watch & yarn storybook",
    "build": "yarn node --loader ts-node/esm ./scripts/build.prod.ts & yarn types",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "serve-storybook": "wrangler dev",
    "deploy-storybook": "wrangler publish"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@cloudflare/wrangler": "^1.19.12",
    "@linaria/babel-preset": "^3.0.0-beta.15",
    "@linaria/esbuild": "^3.0.0-beta.15",
    "@linaria/rollup": "^3.0.0-beta.15",
    "@linaria/shaker": "^3.0.0-beta.15",
    "@linaria/webpack-loader": "^3.0.0-beta.16",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/node-logger": "^6.4.22",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.22",
    "@types/node": "^17.0.25",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@woodshop/builder": "^1.0.0",
    "babel-loader": "^8.2.5",
    "clsx": "^1.1.1",
    "eslint": "8.13.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-storybook": "^0.5.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.70.2",
    "sass": "^1.50.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0"
  },
  "peerDependencies": {
    "@woodshop/icons": "^1.0.0",
    "@woodshop/illustrations": "^1.0.0",
    "clsx": "^1.1.1",
    "framer-motion": "^6.2.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@linaria/core": "^3.0.0-beta.15",
    "@linaria/react": "^3.0.0-beta.15",
    "@woodshop/icons": "^1.0.0",
    "@woodshop/illustrations": "^1.0.0",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.2",
    "framer-motion": "^6.3.0",
    "polished": "^4.2.2",
    "react-scripts": "^5.0.1"
  }
}
