{
  "name": "@woodshop/backend",
  "version": "0.0.1",
  "description": "The Medusa backend store for the Montessori Woodshop storefront.",
  "author": "Drew DeCarme (drew@themontessoriwoodshop.com)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn babel src -d dist --extensions \".ts,.js\"",
    "dev": "yarn medusa develop",
    "start": "yarn medusa start",
    "migrate": "yarn medusa migrations run",
    "serve": "yarn medusa migrations run && yarn medusa start",
    "version": "yarn medusa version"
  },
  "dependencies": {
    "@medusajs/medusa": "^1.1.57",
    "@medusajs/medusa-cli": "^1.1.23",
    "dotenv": "^10.0.0",
    "medusa-fulfillment-manual": "^1.1.26",
    "medusa-interfaces": "^1.1.27",
    "medusa-payment-manual": "^1.0.8",
    "medusa-payment-stripe": "^1.1.30",
    "mongoose": "^5.13.3",
    "pg": "^8.7.1",
    "typeorm": "^0.2.36"
  },
  "repository": "https://github.com/medusajs/medusa-starter-default.git",
  "keywords": [
    "sqlite",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.14.5",
    "babel-preset-medusa-package": "^1.1.13",
    "prettier": "^2.4.1"
  }
