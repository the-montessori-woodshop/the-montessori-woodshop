generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["dataProxy"]
}

datasource db {
  provider          = "postgresql"
  url               = env("WOODSHOP_API_DATABASE_URL")
  shadowDatabaseUrl = env("WOODSHOP_API_SHADOW_DATABASE_URL")
}

model Log {
  id      String @id @default(dbgenerated())
  level   Level
  message String
  meta    Json
}

enum Level {
  Info
  Warn
  Error
}
