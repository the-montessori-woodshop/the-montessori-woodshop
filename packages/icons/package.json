{
  "name": "@woodshop/icons",
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "yarn svgr --config-file=svgr.config.cjs svg",
    "docs": "yarn node ./scripts/create-docs.mjs && prettier --write ./ICONS.md",
