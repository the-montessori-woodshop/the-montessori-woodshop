  },
  "homepage": "https://github.com/machineq/asset-tracker#readme",
  "devDependencies": {
    "@svgr/cli": "^6.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/prettier": "^2.6.3",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@woodshop/builder": "^1.0.0",
    "fs-extra": "^10.1.0",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "typescript": "^4.7.4"
