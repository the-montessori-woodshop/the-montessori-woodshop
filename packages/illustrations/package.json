  "peerDependencies": {
    "react": "17.0.2"
  },
  "devDependencies": {
    "@svgr/cli": "^6.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/prettier": "^2.6.0",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@woodshop/builder": "^1.0.0",
    "fs-extra": "^10.1.0",
