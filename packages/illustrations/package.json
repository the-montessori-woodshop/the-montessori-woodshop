  "bugs": {
    "url": "https://github.com/machineq/asset-tracker/issues"
  },
  "homepage": "https://github.com/machineq/asset-tracker#readme",
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@svgr/cli": "^6.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/prettier": "^2.4.4",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@woodshop/builder": "^1.0.0",
    "fs-extra": "^10.0.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "rollup": "^2.70.0",
    "typescript": "^4.6.2"
  }
