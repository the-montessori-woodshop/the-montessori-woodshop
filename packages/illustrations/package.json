{
  "name": "@woodshop/illustrations",
  "version": "1.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "generate": "yarn svgr --config-file=svgr.config.cjs svg",
    "docs": "yarn node ./scripts/create-docs.mjs && prettier --write ./ICONS.md",
    "types": "yarn tsc",
    "bundle": "rollup --config rollup.config.cjs & yarn types",
    "build": "yarn bundle"
  },
  "targets": {
    "types": false
  },
  "files": [
    "dist/**/*",
    "svg/**/*"
  ],
  "repository": {
    "type": "git",
