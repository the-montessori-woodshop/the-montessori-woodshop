{
  "name": "medusa-api",
  "version": "0.0.1",
  "description": "The Medusa backend store for the Montessori Woodshop storefront.",
  "author": "Drew DeCarme (drew@themontessoriwoodshop.com)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "babel src -d dist --extensions \".ts,.js\"",
    "build:ci": "babel src -d dist --extensions \".ts,.js\"",
    "dev": "medusa develop",
    "start": "medusa start",
    "migrate": "medusa migrations run",
    "serve": "yarn medusa migrations run && yarn medusa start",
    "version": "yarn medusa version",
    "seed": "yarn medusa seed -f ./data/seed.json"
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/preset-typescript": "^7.17.12",
    "@medusajs/medusa": "^1.3.2",
    "@medusajs/medusa-cli": "^1.3.0",
    "babel-preset-medusa-package": "^1.1.20",
    "dotenv": "^16.0.1",
    "medusa-core-utils": "^1.1.31",
    "medusa-file-cloudflare-images": "^1.0.1",
    "medusa-fulfillment-manual": "^1.1.32",
    "medusa-interfaces": "^1.3.0",
    "medusa-payment-manual": "^1.0.17",
    "medusa-payment-stripe": "^1.1.40",
    "mongoose": "^6.4.0",
    "pg": "^8.7.3",
    "query-string": "^7.1.1",
    "typeorm": "^0.3.6"
  },
  "repository": "https://github.com/medusajs/medusa-starter-default.git",
  "keywords": [
    "sqlite",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "engines": {
    "node": "16.13.0"
  }
}
